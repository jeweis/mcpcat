{
  "fetch": {
    "type": "stdio",
    "command": "uvx",
    "args": [
      "mcp-server-fetch"
    ]
  },
  "mayna-openapi": {
    "type": "openapi",
    "spec_url": "https://api.jeweis.com/api/v3/api-docs/default",
    "api_base_url": "https://api.jeweis.com/api",
    "route_configs": [
      {
        "methods": [
          "GET"
        ],
        "pattern": "^/user/protect/userManage/.*"
      }
    ]
  },
  "sequential-thinking": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-sequential-thinking"
    ]
  },
  "time": {
    "type": "stdio",
    "command": "uvx",
    "args": [
      "mcp-server-time",
      "--local-timezone=Asia/Shanghai"
    ],
    "env": {},
    "timeout": 30
  },
  "xiaogj-ai": {
    "type": "openapi",
    "spec_url": "http://nas.jeweis.com:18044/xiaogj-ai-api/v3/api-docs",
    "api_base_url": "http://nas.jeweis.com:18044/xiaogj-ai-api",
    "route_configs": [
      {
        "methods": [
          "GET",
          "POST"
        ],
        "pattern": "^/tool/.*"
      }
    ],
    "timeout": 240
  }
}