{
  "fetch": {
    "type": "stdio",
    "command": "uvx",
    "args": [
      "mcp-server-fetch"
    ]
  },
  "sequential-thinking": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@modelcontextprotocol/server-sequential-thinking"
    ]
  },
  "time": {
    "type": "stdio",
    "command": "uvx",
    "args": [
      "mcp-server-time",
      "--local-timezone=Asia/Shanghai"
    ],
    "env": {},
    "timeout": 30
  }
}